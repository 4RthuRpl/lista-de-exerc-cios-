#include <iostream>

using namespace std;

int main()
{
    float numero1;
    float numero2;
    cout << "Digite o valor do 1° numero: ";
    cin >> numero1;
    cout << "Digite o valor do 2° numero: ";
    cin >> numero2;
    if(numero1>numero2){ 
        cout << "O numero maior é o 1°: " << numero1;
    }else{
        cout << "O numero maior é o 2°: " << numero2;
    }
     return 0;
}
