#include <iostream>
#include <math.h>
using namespace std;

int main()
{   
    int cateto1, cateto2;
    cout << "digite o valor do cateto 1:";
    cin >> cateto1;
    cout << "digite o valor do cateto 2:";
    cin >> cateto2;
    cout << "hipotenusa igual a: " << sqrt(pow(cateto1,2)+pow(cateto2,2));
    return 0;
}

